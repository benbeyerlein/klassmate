<div class="page-header">
  <h1>Student #<%= @student.id %></h1>
</div>

<dl class="dl-horizontal">
  <dt>First name</dt>
  <dd><%= @student.first_name %></dd>

  <dt>Last name</dt>
  <dd><%= @student.last_name %></dd>

  <dt>School userid</dt>
  <dd><%= @student.school_userid %></dd>

  <dt>School username</dt>
  <dd><%= @student.school_username %></dd>

  <dt>School email</dt>
  <dd><%= @student.school_email %></dd>

  <dt>Profile url</dt>
  <dd><%= @student.profile_url %></dd>

  <dt>Avatar url</dt>
  <dd><%= @student.avatar_url %></dd>

</dl>
